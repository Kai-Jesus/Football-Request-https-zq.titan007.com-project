def zp_titan(self,game_id_list):
    
    for i in game_id_list:
        url = 'https://live.titan007.com/detail/'+str(i)+'cn.htm'
        print(url)
        user_agent = {
            'User-Agent':'Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/113.0.0.0 Safari/537.36',
            'Cookie':'Registered=1; Hm_lvt_5ee25051f2fb71cfbf0bfa5bbd459b6f=1692381386; Hm_lpvt_5ee25051f2fb71cfbf0bfa5bbd459b6f=1692381386; fAnalyCookie=null; Hm_lvt_6ae6f47a344eddfeef4d7bfc2cce7742=1692385571; detailCookie=null; Hm_lpvt_6ae6f47a344eddfeef4d7bfc2cce7742=1692386605'
        }

        response = requests.get(url, headers = user_agent).content.decode('utf-8')

        # 解析网页
        soup = BeautifulSoup(response, 'html.parser')
        # 提取ul元素
        ul_content = soup.find('ul', class_='listbox', id='teamEventDiv')
        formatted_lines = []
        formatted_tuples = []

        for li in ul_content.find_all('li'):
            # 提取<a>标签中的文本，并用逗号分隔
            names = [a.get_text() for a in li.find_all('a')]
            name_str = "，".join(names)

            # 获取<li>标签的完整文本
            line = li.get_text(strip=True)

            # 检查是否有黄牌或红牌的图像，并添加相关的描述
            card = ""
            if li.find('img', {'title': '黄牌'}):
                card = "黄牌"
            elif li.find('img', {'title': '紅牌'}):  # 注意这里使用的是繁体字
                card = "红牌"

            # 根据原有逻辑进行格式化
            if line:
                formatted_line = name_str
                if card:
                    formatted_line += f" ({card})"
                if line[-1] == "'":
                    formatted_tuples.append((formatted_line, line))
                else:
                    idx = line.find("'")
                    if idx != -1:
                        formatted_tuples.append((formatted_line, line[idx + 1:] + line[:idx + 1]))
                    else:
                        formatted_tuples.append((formatted_line, line))

        # 打印格式化后的元组
        for formatted_tuple in formatted_tuples:
            print(formatted_tuple)


        # 提取第一个元组的第二个元素
        teams_string = formatted_tuples[0][1]

        # 使用正则表达式找到两个数字之间的所有字符
        match = re.search(r'(\D+)(\d)(\D+)(\d)', teams_string)

        if match:
            home_team = match.group(1) + match.group(2)
            away_team = match.group(3) + match.group(4)

            # 替换第一个元组
            formatted_tuples[0] = (home_team.strip(), away_team.strip())

#         print(formatted_tuples)  # 结果为 [('皇家贝蒂斯1', '巴伦西亚1'), ...]

        for idx, tup in enumerate(formatted_tuples):
            # 使用正则表达式匹配时间
            match = re.search(r"(\d+['+]*\d*')", tup[1])
            if match:
                time_only = match.group(1)
                formatted_tuples[idx] = (tup[0], time_only)

        print(formatted_tuples)
  
zp_titan(game_id_list)



