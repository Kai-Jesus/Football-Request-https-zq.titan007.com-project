# 步骤1: 替换所有的连续逗号为', None,'
data_str = re.sub(r',\s*,', ', None,', data_str)

# 步骤2: 使用正则表达式匹配
pattern = r'jh\["R_\d+"\] = (\[\[.*?\]\]);'
matches = re.findall(pattern, data_str)

all_data = []
for match in matches:
    # 使用eval来转换字符串为Python列表
#     data_list = eval(match)
#     print(match)
    all_data.extend(data_list)

game_id = []
# 打印结果
for data in all_data:
    game_id.append(data[0])

print(game_id)
# 当你运行上述代码时，应该可以得到你所期望的结果。

另一个版本
# 使用正则表达式匹配
pattern = r'jh\["R_\d+"\] = (\[\[.*?\]\]);'
matches = re.findall(pattern, data_str)

all_data = []
for match in matches:
    # 使用eval来转换字符串为Python列表
    data_list = eval(match)
    all_data.extend(data_list)

game_id = []
# 打印结果
for data in all_data:
#     print(data[0])
    game_id.append(data[0])

print(game_id)


